<div class="container content">
  <h1>Dashboard</h1>
  <div class="row justify-content-between">

    <%= render 'shared/listed-jobs', jobs: current_user.jobs %>
    <%= render 'shared/closed-jobs', jobs: current_user.jobs %>

  </div>
  <div class="p-2">
    <%= link_to 'Change password', registration_path(current_user), :method => :patch %>
  </div>
  <div class="p-2">
    <%= link_to 'Update', edit_user_path(current_user) %>
  </div>
</div>
