
  <div class="card m-2 p-3">
  <div class="d-flex justify-content-between">
    <h2>Listed Jobs</h2>
    <%= link_to "Create Job", new_job_path, class: "btn btn-ghost" %>
  </div>
    <br>

    <% jobs.each do |job| %>
      <% unless job.done %>
        <%= link_to job.title, job_path(job) %>
      <% end %>
    <% end %>
  </div>

