<div id="closed-jobs" class="col-12 d-none profile-card">
  <div class="card p-3 mx-2">
  <div class="d-flex justify-content-between">
    <h2>Closed Jobs</h2>
  </div>
    <br>

    <% jobs.each do |job| %>
      <% if job.done %>
        <%= link_to job.title, job_path(job) %>
      <% end %>
    <% end %>
  </div>
</div>
